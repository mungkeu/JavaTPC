package kr.bit;

public class TPC05 {
	// 9개의 정수형 변수를 만들어라
	int[][] b=new int[3][3];
	b[0][0]=1;
	b[0][1]=1;
	b[0][2]=3;
	
	b[1][0]=1;
	b[1][1]=1;
	b[1][2]=3;
	
	b[2][0]=1;
	b[2][1]=1;
	b[2][2]=3;
	
	for(int i=0; i<b.length; i++)
	{
		for(int j=0; j<b[i].length; j++)
		{
			System.out.println(b[i][j]);
		}
	}
}
